<% content_for :title do %>HADashboard<% end %>

<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="clock" data-view="Clock"></div>
    </li>

    <li data-row="1" data-col="3" data-sizex="2" data-sizey="2">
      <div data-id="weather" data-view="Stweather"></div>
    </li>

    <li data-row="1" data-col="5" data-sizex="2" data-sizey="1">
      <div data-id="mode" data-view="Stmode" data-title="Current Mode"></div>
    </li>

    <li data-row="2" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="temp" data-view="Sttemp" data-title="Outside Temperature" data-device="Porch Multisensor"></div>
    </li>

    <li data-row="2" data-col="5" data-sizex="1" data-sizey="1">
      <div data-id="presence1" data-view="Stpresence" data-title="Andrew" data-device="Andrew's iPhone"></div>
    </li>

    <li data-row="2" data-col="6" data-sizex="1" data-sizey="1">
      <div data-id="presence2" data-view="Stpresence" data-title="Wendy" data-device="Wendy's iPhone"></div>
    </li>

    <li data-row="2" data-col="7" data-sizex="1" data-sizey="1">
      <div data-id="garage" data-view="Stcontact" data-title="Garage" data-device="Garage"></div>
    </li>

    <li data-row="1" data-col="7" data-sizex="1" data-sizey="1">
      <div data-id="pmotion" data-view="Stmotion" data-title="Porch" data-device="Porch Multisensor"></div>
    </li>

    <li data-row="3" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="dson" data-view="Stswitch" data-icon="lightbulb-o" data-title="Downstairs On" data-device="Downstairs On" data-event-click="onClick"></div>
    </li>

	<li data-row="3" data-col="3" data-sizex="2" data-sizey="1">
      <div data-id="dsoff" data-view="Stswitch" data-icon="lightbulb-o" data-title="Downstairs Off" data-device="Downstairs Off" data-event-click="onClick"></div>
    </li>

	<li data-row="3" data-col="5" data-sizex="2" data-sizey="1">
      <div data-id="dsbright" data-view="Stswitch" data-icon="lightbulb-o" data-title="Downstairs Bright" data-device="Downstairs Bright" data-event-click="onClick"></div>
    </li>

	<li data-row="3" data-col="7" data-sizex="1" data-sizey="1">
      <div data-id="gopen" data-view="Stswitch" data-icon="lightbulb-o" data-title="Garage" data-device="Garage" data-event-click="onClick"></div>
    </li>

    <li data-row="4" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="morningmode" data-view="Stmodechange" data-title="Good Morning" data-icon="sun-o" data-changemode="Day" data-phrase="Sunrise" data-event-click="onClick"></div>
    </li>

	<li data-row="4" data-col="3" data-sizex="2" data-sizey="1">
      <div data-id="eveningmode" data-view="Stmodechange" data-title="Good Evening" data-icon="moon-o" data-changemode="Evening" data-phrase="Sunset" data-event-click="onClick"></div>
    </li>
	
    <li data-row="4" data-col="5" data-sizex="2" data-sizey="1">
      <div data-id="nightmode" data-view="Stmodechange" data-title="Good Night" data-icon="star-o" data-changemode="Night" data-phrase="Bedtime" data-countdown="30" data-event-click="onClick"></div>
    </li>

	<li data-row="4" data-col="7" data-sizex="1" data-sizey="1">
      <div data-id="ddimmer" data-view="Stdimmer" data-icon="lightbulb-o" data-title="Downstairs Dimmer" data-device="Downstairs Dimmer" data-event-click="onClick"></div>
    </li>
	
	<li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="alamp" data-view="Stdimmer" data-icon="lightbulb-o" data-title="Andrew's Lamp" data-device="A Den" data-event-click="onClick"></div>
    </li>

	<li data-row="1" data-col="2" data-sizex="1" data-sizey="1">
      <div data-id="wlamp" data-view="Stswitch" data-icon="lightbulb-o" data-title="Wendy's Lamp" data-device="W Den" data-event-click="onClick"></div>
    </li>

	<li data-row="1" data-col="3" data-sizex="1" data-sizey="1">
      <div data-id="lrlamp" data-view="Stdimmer" data-icon="lightbulb-o" data-title="Living Room Lamp" data-device="Living Room Lamp" data-event-click="onClick"></div>
    </li>

	<li data-row="1" data-col="4" data-sizex="1" data-sizey="1">
      <div data-id="hlamp" data-view="Stdimmer" data-icon="lightbulb-o" data-title="Hall Light" data-device="Hall Light" data-event-click="onClick"></div>
    </li>

	<li data-row="1" data-col="5" data-sizex="1" data-sizey="1">
      <div data-id="plamp" data-view="Stdimmer" data-icon="lightbulb-o" data-title="Porch Light" data-device="Porch Light" data-event-click="onClick"></div>
    </li>

    <li data-row="4" data-col="10" data-sizex="1" data-sizey="1">
      <div data-id="reload" data-view="Reload" data-event-click="onClick"></div>
    </li>
	
  </ul>
</div>
